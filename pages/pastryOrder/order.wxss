/* pages/orderList/orderList.wxss */

page {
  background-color: #f1f1f1;
  color: #333;
}


/* pages/order/order.wxss */
page,.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #f5f5f5;
}
.models-nav{
  display: flex;
  flex-direction: row;
  color: #999;
  font-size: 28rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #e5e5e5;
}
.models-item{
  flex: 1;
  text-align: center;
}
.models-item text{
  display: inline-block;
  padding: 30rpx 0;
  border-bottom: 4rpx solid transparent;
  box-sizing: border-box;
  color: #333; 
}
.models-item text.active{
  border-bottom: 6rpx solid #5fb878;
  color: #009688;
  width: 125rpx;
}
.order-no{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #f9f9f9;
}
.order-no image{
  width: 200rpx;
  height: 200rpx;
}
.order-no text{
  margin-top: 50rpx;
  font-size: 24rpx;
  color: #aaa;
}

/* 列表 */
.scroll-view{
  flex: 1;
  overflow: hidden;
}
.order-list{
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}
.order-item{
  display: flex;
  flex-direction: column;
  padding: 40rpx 40rpx 30rpx;
  font-size: 28rpx;
  background-color: #fff;
  margin:0 40rpx 20rpx;
  border-radius: 20rpx;
}
.order-item-addr{
  display: flex;
  flex-direction: row;
  align-items: center;
  overflow: hidden;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.order-item-addr .order-item-title{
  flex: 1;
  font-size: 32rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.order-item-pro-content{
  margin-left: 60rpx;
}
.order-item-pro{
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 10rpx;
  font-size: 28rpx;
  color: #444;
}
.order-item-img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 10rpx;
}
.order-item-detail{
  margin-left: 20rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.order-item-detail-romm{
  margin-bottom: 10rpx;
}
.order-item-addr .color1dffa1{
  color: #13d896;
}
.order-item-addr .color000{
  color: #000;
}
.order-item-addr .colorff961e{
  color: #ff961e;
}
.order-item-total{
  padding-top: 20rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-top: 1rpx solid #e5e5e5;
}
.order-item-price{
  margin-right: 10rpx;
  font-size: 28rpx;
}
.order-item-price text{
  color: #eb2444;
}
.order-item-btn{
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  margin-top: 20rpx;
}
.order-item-btn1{
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.order-item-btn text{
  padding: 8rpx 25rpx;
  margin-left: 10rpx;
  font-size: 24rpx;
  border-radius: 28rpx;
  border: 1rpx solid transparent;
}
.order-item-btn text.cancel{
  border-color:#ddd;
  color: #aaa;
}

.order-item-btn text.cancel1{
  border-color:#ddd;
  color: #aaa;
}
.order-item-btn text.pay{
  border-color:#ff961e;
  color: #ff961e;
}
.goods{
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
}
.goods-list{
  display: flex;
  flex-direction: column;
  overflow: hidden;
  position: relative;
}
.goods-time{
  font-size: 28rpx;
  padding-top: 10rpx;
  color: #666;
}

.goods-list .item-cont {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
  /* padding: 30rpx; */
  /* border-bottom: 2rpx solid #f1f1f1; */
}
.price-nums{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 24rpx;
}
.goods-list .item-cont .prod-pic {
  font-size: 0;
  /* display: block; */
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  background: #fff;
  margin-right: 16rpx;
}

.goods-list .item-cont .prod-pic image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.goods-list .item-cont .prod-info {
  margin-left: 10rpx;
  font-size: 28rpx;
  flex: 1;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 160rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
  -webkit-flex: 1;
  -ms-flex: 1;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
}
.goods-list .item-cont .prod-info .prodname {
  font-size: 30rpx;
  line-height: 40rpx;
  max-height: 86rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
  color: #333;
}

.goods-list .item-cont .prod-info .prod-info-cont {
  color: #999;
  line-height: 40rpx;
  margin-top: 10rpx;
  font-size: 22rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
}



.popup{
  z-index: 1;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0rpx;
  display: flex;
  flex-direction: column;
}

.price-detail-model1{
  height: 350rpx;
  background: rgba(0, 0, 0, .4);
}
.price-detail1{
  flex: 1;
  overflow: hidden;
  background-color: #fff;
  display: flex;
  flex-direction: column;
}

.price-detail-title1 image{
  width: 30rpx;
  height: 30rpx;
  padding: 40rpx;
}

.price-detail-title1{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-right: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
/* 支付方式 */
.pay-item{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 25rpx 40rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.pay-item-con text{
  font-size: 34rpx;
  color: #fb6600;
  font-weight: 700;
}
.pay-item view.pay-item-con{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.pay-item-price{
  color: #333;
}
.pay-item image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
.prod-inp{
  margin: 30rpx;
}
.prod-inp-label{
  font-size: 30rpx;
  font-weight: 500;
  color: #333;
  display: block;
  float: right;
  margin-top: -14rpx;
  margin-right: 39rpx;

}

/* 按钮 */
.hover-class{
  width: 90%;
