.room-detail-box{
  z-index: 99999;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 0 40rpx;
  background: rgba(0, 0, 0, .5);
}
.room-detail{
  width: 100%;
  max-height: 80%;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  padding-bottom: 50rpx;
  padding-top: 25rpx;
}
.room-detail-swiper{
  width: 100%;
  height: 400rpx;
  background-color: #fff;
}
.room-detail-swiper-img{
  width: 100%;
  height: 100%;
}
.room-detail-con{
  margin-top: 10rpx;
  flex: 1;
  padding: 0 30rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.room-detail-title{
  text-align: center;
  padding: 20rpx 0;
