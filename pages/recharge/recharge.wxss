page{
  background: #f2f2f2;
  height: 100%;
  width: 100%;
}
.title{
  position: sticky;
  top: 0;
  z-index: 10;
  display: flex;
  line-height: 80rpx;
}
.username{
  flex: 1;
  text-align: center;
  color: #cacaca;
  font-size: 26rpx;
}

.list{
  display: flex;
  line-height: 60rpx;
  font-size: 24rpx;
  padding: 25rpx 0;
  border-bottom: 1px solid #ebebeb;
}
.list-desc{
  flex: 1;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list-price{
  color: #f55b02;
  font-weight: bold;
  font-size: 36rpx;
}

.order-no{
  position: absolute;
  left: 50%;
  top: 20%;
  margin-left: -100rpx;
}
.order-no image{
  width: 200rpx;
  height: 200rpx;
}
.order-no .noitem{
  color: #999;
  font-size: 24rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}